{
    "name": "yranarf/tomatoidc",
    "description": "A new idc server",
    "keywords": ["framework", "laravel"],
    "license": "GPL3",
    "type": "project",
    "require": {
        "php": "^7.1.3",
        "arcanedev/log-viewer": "^4.6",
        "beyondcode/laravel-self-diagnosis": "^1.0",
        "fideloper/proxy": "^4.0",
        "guzzlehttp/guzzle": "^6.3",
        "intervention/image": "^2.4",
        "johnlui/aliyun-oss": "~2.0",
        "laravel/framework": "5.7.*",
        "laravel/tinker": "^1.0",
        "overtrue/easy-sms": "^1.1",
        "overtrue/laravel-lang": "~3.0",
        "overtrue/laravel-mail-aliyun": "dev-master",
        "overtrue/laravel-wechat": "~4.0",
        "overtrue/laravel-youzan": "^2.0",
        "simplesoftwareio/simple-qrcode": "~2",
        "thomaswelton/laravel-gravatar": "~1.0",
        "yansongda/laravel-pay": "^2.0",
        "ybr-nx/laravel-mariadb": "^1.0"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0",
        "barryvdh/laravel-debugbar": "^3.2"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ],
        "post-install-cmd":[
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\"",
            "@php artisan key:generate"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": {
        "packagist": {
            "type": "composer",
            "url": "https://packagist.phpcomposer.com"
        }
    }
}
